        <AuxValue name="JavaCodeGenerator_CreateCodeCustom" type="java.lang.String" value="new javax.swing.JPanel() {&#xa;&#xa;    public void paintComponent(Graphics g) {&#xa;        super.paintComponent(g);&#xa;&#xa;        g.setColor(Color.BLACK);&#xa;        int pointsize = 7;&#xa;        int offset = pointsize/2;&#xa;        for (Curve curve : draftform.getCurves()) {&#xa;&#xa;            g.fillOval(&#xa;                (int) curve.getStart().x - offset,&#xa;                (int) curve.getStart().y - offset,&#xa;                pointsize, pointsize);&#xa;            g.fillOval(&#xa;                (int) curve.getEnd().x - offset,&#xa;                (int) curve.getEnd().y - offset,&#xa;                pointsize, pointsize);&#xa;&#xa;            Vec2[] points = curve.linearize(curve.recommendedSubdivisions());&#xa;&#xa;            for (int i = 1; i &lt; points.length; i++) {&#xa;&#xa;                g.drawLine(&#xa;                    (int) points[i-1].x,&#xa;                    (int) points[i-1].y,&#xa;                    (int) points[i].x,&#xa;                    (int) points[i].y);&#xa;            }&#xa;&#xa;            points = curve.getControlPoints();&#xa;&#xa;            for (int i = 0; i &lt; points.length; i++) {&#xa;&#xa;                g.drawOval(&#xa;                    (int) points[i].x - offset,&#xa;                    (int) points[i].y - offset,&#xa;                    pointsize, pointsize);&#xa;            }&#xa;        }&#xa;&#xa;        for (Vertex vert : draftform.getVerts()) {&#xa;&#xa;            g.fillOval(&#xa;                (int) vert.x - offset,&#xa;                (int) vert.y - offset,&#xa;                pointsize, pointsize);&#xa;        }&#xa;&#xa;        g.setColor(Color.BLUE);&#xa;        for (Vertex vert : toolkit.getSelectedVerts()) {&#xa;&#xa;            g.fillOval(&#xa;                (int) vert.x - offset,&#xa;                (int) vert.y - offset,&#xa;                pointsize, pointsize);&#xa;        }&#xa;    }&#xa;}"/>
